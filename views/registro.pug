extends layouts/head

block contenido

    main.container.py-4(style="min-height: 80vh")
        .row 
            .col 
                form.row.g-3
                    .col-md-6.col-12
                        label.visually-hidden(for='staticEmail2') Email
                        input#staticEmail2.form-control(type='text' value='' placeholder="E-mail")
